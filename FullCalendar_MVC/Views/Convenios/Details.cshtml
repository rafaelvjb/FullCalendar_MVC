@model FullCalendar_MVC.Models.Convenio

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 class="col-md-offset-5" style="padding-left:45px">Detalhes</h2>
<div class="row">
    <div class="col-md-offset-3 col-md-6">
        <hr />
        <dl class="dl-horizontal col-md-offset-2">
            <dt>
                @Html.DisplayNameFor(model => model.Nome)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Nome)
            </dd>
        </dl>
    </div>
    <p class="col-md-offset-5 col-md-5" style="padding-left:70px;">
        @Html.ActionLink("Editar", "Edit", new { id = Model.ConvenioId })
    </p>
</div>